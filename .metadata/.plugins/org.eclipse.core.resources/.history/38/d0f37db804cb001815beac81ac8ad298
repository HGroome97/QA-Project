
public class Iteration {
	public static void main(String[] args) {
		printLoop(10, 20);
		articulateNum();
	}

	public static void printLoop(int num1, int num2) {
		for (int i = num1; i <= num2; i++) {
			System.out.println(i);
		}
	}

	/*
	 * public static String articulateNum(int num) { switch (num) { case 1: return
	 * "one"; case 2: return "two"; case 3: return "three"; case 4: return "four";
	 * case 5: return "five"; case 6: return "six"; case 7: return "seven"; case 8:
	 * return "eight"; case 9: return "nine"; case 10: return "ten"; default:
	 * System.out.println("Invalid Syntax"); return 0; }
	 * 
	 * }
	 */
	public static void articulateNum() {
		//String wordNum;
		String[][] oneToNineteen = new String[19][2];
		//String[][] elevenToNineteen = new String[9][2];
		String[][] multipleOfTens = new String[8][2];
		
		for(int i=1; i<=19; i++) {
			System.out.println(Integer.toString(i));
			oneToNineteen[i][1] = Integer.toString(i);
		}
		oneToNineteen[1][2] = "one";
		oneToNineteen[2][2] = "two";
		oneToNineteen[3][2] = "three";
		oneToNineteen[4][2] = "four";
		oneToNineteen[5][2] = "five";
		oneToNineteen[6][2] = "six";
		oneToNineteen[7][2] = "seven";
		oneToNineteen[8][2] = "eight";
		oneToNineteen[9][2] = "nine";
		oneToNineteen[10][2] = "ten";
		oneToNineteen[11][2] = "eleven";
		oneToNineteen[12][2] = "twelve";
		oneToNineteen[13][2] = "thirteen";
		oneToNineteen[14][2] = "fourteen";
		oneToNineteen[15][2] = "fifteen";
		oneToNineteen[16][2] = "ten";
		oneToNineteen[17][2] = "ten";
		oneToNineteen[18][2] = "ten";
		oneToNineteen[19][2] = "ten";
		
		System.out.println(oneToNineteen[9][2]);
		System.out.println(oneToNineteen[6][1]);
		
		
//		if(num<=10) {
//			
//		}
		
		
	}

}
