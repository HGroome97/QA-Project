package com.qa.Library;

import java.util.ArrayList;
import java.util.List;

public class Library {
	private List<Object> itemList;
	private List<Object> customerList;
	

	public Library() {
		itemList = new ArrayList<Object>();
		customerList = new ArrayList<Object>();
	}

	public List<Object> returnItemList() {
		return itemList;
	}
	public void addItem(List<Object> itemList) {
		this.itemList = itemList;
	}

	
	public List<Object> returnCustomerList() {
		return customerList;
	}

	public void addCustomer(List<Object> customerList) {
		this.customerList = customerList;
	}
	
}
