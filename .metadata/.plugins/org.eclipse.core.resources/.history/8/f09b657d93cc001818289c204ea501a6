package com.qa.GarageProject;

public class Unicycle extends Vehicle{
	public int frameSize;
	
	
	public Unicycle(int frameSize, int price){
		super(price);
		this.frameSize = frameSize;
	}
	public void drive() {}
	public void reverse() {}
	public void park() {}
	public void doTricks() {}

	public String toString() {
		return this.get() + " " + this.getAge() + " " + this.getJobTitle();
	}
	

}
