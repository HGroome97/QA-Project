package com.qa.PaintWizard;

public class Paint {
	private int size;
	private int price;
	private int coverage;
	
	public Paint(int size, int price, int coverage) {
		this.setSize(size);
		this.setPrice(price);
		this.setCoverage(coverage);
	}

	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}

	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {

		this.price = price;
	}
	
	
	public int getCoverage() {
		return coverage;
	}
	public void setCoverage(int coverage) {
		this.coverage = coverage;
	}
}
