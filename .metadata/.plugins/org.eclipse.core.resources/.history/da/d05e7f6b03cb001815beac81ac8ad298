		for(int i=1; i<=10; i++) {
			System.out.println(Integer.toString(i));
			oneToTen[i][1] = Integer.toString(i);
		}
		oneToTen[1][2] = "one";
		oneToTen[2][2] = "two";
		oneToTen[3][2] = "three";
		oneToTen[4][2] = "four";
		oneToTen[5][2] = "five";
		oneToTen[6][2] = "six";
		oneToTen[7][2] = "seven";
		oneToTen[8][2] = "eight";
		oneToTen[9][2] = "nine";
		oneToTen[10][2] = "ten";
				
		System.out.println(oneToTen[9][2]);
		System.out.println(oneToTen[6][1]);
		
