
public class Iteration {
	public static void main(String[] args) {
		printLoop(10, 20);
		articulateNum();
	}

	public static void printLoop(int num1, int num2) {
		for (int i = num1; i <= num2; i++) {
			System.out.println(i);
		}
	}

	/*
